{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run prisma:migrate:up && npm run prisma:generate && nest start",
    "start:watch": "npm run prisma:migrate:up && npm run prisma:silent-watch && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prisma": "prisma",
    "prisma:db:create": "npm run prisma migrate dev",
    "prisma:migrate:up": "npm run prisma migrate deploy",
    "prisma:generate": "npm run prisma generate",
    "prisma:watch": "npm run prisma generate -- --watch",
    "prisma:silent-watch": "npm run prisma generate -- --watch &"
  },
  "dependencies": {
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/graphql": "7.10.3",
    "@nestjs/platform-express": "7.6.15",
    "@prisma/client": "^2.19.0",
    "@prisma/sdk": "^2.19.0",
    "apollo-server-express": "2.22.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "graphql": "15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-subscriptions": "1.2.1",
    "graphql-type-json": "^0.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.6",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@prisma/generator-helper": "2.19.0",
    "@types/express": "4.17.11",
    "@types/graphql-fields": "^1.3.3",
    "@types/node": "14.14.37",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "dataloader": "^2.0.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "graphql-scalars": "^1.9.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prisma": "^2.19.0",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typegraphql-prisma-nestjs": "^0.1200.0",
    "typescript": "4.2.3"
  }
}
