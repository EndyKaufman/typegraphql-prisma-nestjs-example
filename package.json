{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prisma:migrate:up && npm run prisma:generate && nest start",
    "start:watch": "npm run prisma:migrate:up && npm run prisma:silent-watch && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prisma": "prisma",
    "prisma:db:create": "npm run prisma migrate save -- --name 'init' --experimental",
    "prisma:migrate:up": "npm run prisma migrate up -- --experimental",
    "prisma:generate": "npm run prisma generate",
    "prisma:watch": "npm run prisma generate -- --watch",
    "prisma:silent-watch": "npm run prisma generate -- --watch &"
  },
  "dependencies": {
    "@nestjs/common": "7.4.2",
    "@nestjs/core": "7.4.2",
    "@nestjs/graphql": "7.6.0",
    "@nestjs/platform-express": "7.4.2",
    "apollo-server-express": "2.17.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "graphql": "15.3.0",
    "graphql-subscriptions": "1.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.2",
    "type-graphql": "^1.0.0",
    "@prisma/client": "^2.5.1",
    "@prisma/generator-helper": "^2.0.0-alpha.1154",
    "@prisma/sdk": "^2.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.1",
    "@nestjs/schematics": "7.1.1",
    "@nestjs/testing": "7.4.2",
    "@prisma/cli": "^2.5.1",
    "@prisma/generator-helper": "2.5.1",
    "@types/express": "4.17.7",
    "@types/graphql-fields": "^1.3.3",
    "@types/node": "14.6.1",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "dataloader": "^2.0.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-loader": "7.0.4",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typegraphql-prisma-nestjs": "^0.500.0",
    "typescript": "3.9.2"
  }
}
