{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run prisma:migrate:up && npm run prisma:generate && nest start",
    "start:watch": "npm run prisma:migrate:up && npm run prisma:silent-watch && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prisma": "prisma",
    "prisma:db:create": "npm run prisma -- migrate dev",
    "prisma:migrate:up": "npm run prisma -- migrate deploy",
    "prisma:generate": "npm run prisma -- generate",
    "prisma:watch": "npm run prisma -- generate -- --watch",
    "prisma:silent-watch": "npm run prisma -- generate -- --watch &"
  },
  "dependencies": {
    "@nestjs/common": "8.2.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/platform-express": "8.2.6",
    "@prisma/client": "~4.5.0",
    "@prisma/sdk": "~4.0.0",
    "apollo-server-express": "2.25.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "graphql": "15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-subscriptions": "1.2.1",
    "graphql-type-json": "^0.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.1.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.8",
    "@nestjs/schematics": "8.0.6",
    "@nestjs/testing": "8.2.6",
    "@prisma/generator-helper": "~4.5.0",
    "@types/express": "4.17.12",
    "@types/graphql-fields": "^1.3.3",
    "@types/node": "15.6.1",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "dataloader": "^2.0.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.3",
    "graphql-scalars": "^1.9.3",
    "jest": "27.0.1",
    "prettier": "2.3.0",
    "prisma": "~4.5.0",
    "supertest": "6.1.3",
    "ts-jest": "27.0.1",
    "ts-loader": "9.2.2",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "typegraphql-prisma-nestjs": "0.2200.1",
    "typescript": "4.3.5"
  }
}
